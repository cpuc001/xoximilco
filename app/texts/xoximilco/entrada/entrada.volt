body,p{color:#353a39}*,:after,:before,html{box-sizing:border-box}body,html{overflow-x:hidden}body,h1,html,p{margin:0;padding:0}html{font-size:62.5%}body{font-size:1.6em;line-height:1.6;font-weight:400;font-family:Arial,Helvetica,sans-serif;background-image:-webkit-linear-gradient(top,#fdf5e6 0,#fff 70%);background-image:-moz-linear-gradient(top,#fdf5e6 0,#fff 70%);background-image:-o-linear-gradient(top,#fdf5e6 0,#fff 70%);background-image:-ms-linear-gradient(top,#fdf5e6 0,#fff 70%);background-image:linear-gradient(to bottom,#fdf5e6 0,#fff 70%)}.backdrop{height:113px;display:block}h1{color:#f09;margin-bottom:0;font-weight:700;font-size:3rem;line-height:1.2;font-family:fuente-xox}p{margin-top:0;margin-bottom:2.5rem;font-size:1.6rem}.carousel-indicators{margin:auto;position:absolute;text-align:center;bottom:10px;left:0;list-style:none;padding-left:0;right:0;width:80%;z-index:15}.booking{background-color:#fff;max-width:990px;z-index:50}