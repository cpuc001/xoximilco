.glyphicon
	position relative
	top 1px
	display inline-block
	font-family 'iconexx'
	font-style normal
	font-weight normal
	line-height 1
	-webkit-font-smoothing antialiased
	-moz-osx-font-smoothing grayscale

.glyphicon-time
	&:before
		content "\e023"

.glyphicon-chevron-left
	&:before
		content "\e02a"

.glyphicon-chevron-right
	&:before
		content "\e029"

.glyphicon-chevron-up
	&:before
		content "\e02c"

.glyphicon-chevron-down
	&:before
		content "\e02b"

.glyphicon-calendar
	&:before
		content "\e014"

.glyphicon-shopping-cart
	&:before
		content "\e010"

